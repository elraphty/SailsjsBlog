<h1 class="display-4">Articles</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Title</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% articles.forEach(function(article) { %>
      <tr>
        <td> <%= article.id %> </td>
        <td> <%= article.title %> </td>
        <td></td>
      </tr>
    <% }) %>
  </tbody>
</table>